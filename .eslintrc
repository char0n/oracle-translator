{
  "extends": "google",
  "ecmaFeatures": {

  },
  "rules": {
    "no-warning-comments": [1, { "terms": ["fixme", "xxx"], "location": "start" }],
    "max-len": [1, 150],
    "spaced-comment": [0, "never"],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "array-callback-return": 0,
    "new-cap": 0,
    "require-jsdoc": 0,
    "linebreak-style": 0,
    "valid-jsdoc": 0,
    "arrow-parens": 0,
    "max-nested-callbacks": [1, 5],
    "dot-notation": 1,
    "no-debugger": 1
  },
  "globals": {
    "angular": true,
    "window": true,
    "document": true,
    "event": true,
    "define": true,
    "jQuery": true,
    "WebSocket": true,
    "$": true,
    "moment": true,
    "_": true,
    "that": true,
    "fetch": true,
    "describe": true,
    "it": true,
    "beforeEach":  true,
    "afterEach": true,
    "inject": true,
    "sinon": true,
    "NODE_ENV": true
  }
}
